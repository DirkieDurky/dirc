// Test break statement
for (int i = 0; i < 10; i++) {
    if (i == 5) {
        break;
    }
    print(intToString(i));
}

// Test continue statement
for (int j = 0; j < 5; j++) {
    if (j == 2) {
        continue;
    }
    print(intToString(j));
}

// Test break in while loop
int k = 0;
while (k < 10) {
    if (k == 3) {
        break;
    }
    print(intToString(k));
    k++;
}

// Test continue in while loop
int m = 0;
while (m < 5) {
    m++;
    if (m == 3) {
        continue;
    }
    print(intToString(m));
}
