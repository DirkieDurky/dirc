char* concat(char* a, char* b)
{
    int aLen = strlen(a);
    int bLen = strlen(b);
    int size = aLen + bLen + 1;

    char* newString = malloc(size);

    for (int i = 0; i < aLen; i++)
    {
        newString[i] = a[i];
    }

    for (int i = 0; i < bLen; i++)
    {
        newString[aLen + i] = b[i];
    }
    newString[size - 1] = 0;

    return newString;
}
