int stringToInt(char[] str)
{
    int result = 0;

    int length = strlen(str);
    char c;
    int digitCount = 0;
    for (int i = length; i >= 0; i--)
    {
        c = str[i];
        if (c >= 48 & c <= 57)
        {
            int digit = c - 48;
            result += digit * power(10, digitCount);
            digitCount++;
        }
    }

    return result;
}
